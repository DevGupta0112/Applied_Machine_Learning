{
  "version": 2,
  "builds": [
    { "src": "stock_prediction/api/predict.py", "use": "@vercel/python" },
    { "src": "stock_prediction/package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "stock_prediction/api/predict.py" },
    { "src": "/(.*)", "dest": "stock_prediction/" }
  ]
}
